var Index = {
    initFunc: function() {
        var self = this;
        $('#navBar>li:nth-child(1)').addClass('active');
        self.clickFunc();
        self.checkLoginFunc();
        self.swiperFunc();
    },

    clickFunc: function() {
        $('div#choice').hover(function() {
            var self = this;
            if (!$(self).hasClass("choiceActive")) {
                $('div#choice').removeClass("choiceActive");
                $(self).addClass('choiceActive');
            }
        });

        $('div#choice').mouseleave(function() {
            var self = this;
            $(self).removeClass('choiceActive');
        });

        $('div#choice').on('click', function() {
            if (Utils.checkLogin()) {
                Web.go('index_displayPage.html');
            } else {
                if ($(this).attr('data-new') == '1') {
                    Web.go('newPeopleGuide.html');
                } else {
                    Web.go('index_register.html');
                }
            }
        });
    },

    checkLoginFunc: function() {
        if (Utils.checkLogin()) {
            $('div#bannerBox').hide();
        }
    },

    swiperFunc: function() {
        var mySwiper = new Swiper('.swiper-container', {
            pagination: '.swiper-pagination',
            paginationClickable :true,
            autoplay: 3000,
        });
    }
};
